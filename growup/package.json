{
  "name": "growup",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "prestart": "node scripts/write-env.js",
    "start": "ng serve",
    "build": "ng build",
    "build:gh": "ng build --configuration production --base-href /GrowUp/",
    "deploy": "npm run build:gh && npx angular-cli-ghpages --dir=dist/growup/browser",
    "supabase:push": "supabase db push",
    "supabase:reset": "supabase db reset",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.0",
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/cdk": "^21.1.3",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@angular/service-worker": "^21.1.3",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@supabase/supabase-js": "^2.51.0",
    "ag-grid-angular": "^35.0.1",
    "ag-grid-community": "^35.0.1",
    "dexie": "^4.0.10",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@angular/material": "^21.1.3",
    "gh-pages": "^6.3.0",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
